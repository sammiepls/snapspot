<%= form.hidden_field :user_id, value: current_user.id %>

<div class="text-field">
  <%= form.text_field :name, type: 'text', placeholder: "Name" %>
</div>
<div class="text-field">
  <%= form.text_area :description, type: 'text_area', placeholder: "Description" %>
</div>
<div class="text-field">
  <%= form.text_field :address, type: 'text', placeholder: "Address" %>
</div>
<div class="text-field">
  <%= form.text_field :latitude, type: 'text', placeholder: "Latitude" %>
</div>
<div class="text-field">
  <%= form.text_field :longitude, type: 'text', placeholder: "Longitude" %>
</div>
